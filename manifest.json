{
  "manifest_version": 2,
  "name": "双阶计算器插件",
  "version": "1.0",
  "description": "包含两个相关计算器的Edge插件",
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/calculator.png",
      "32": "icons/calculator.png",
      "48": "icons/calculator.png",
      "128": "icons/calculator.png"
    }
  },
  "icons": {
    "16": "icons/calculator.png",
    "32": "icons/calculator.png",
    "48": "icons/calculator.png",
    "128": "icons/calculator.png"
  }
}